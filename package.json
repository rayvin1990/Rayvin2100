{
  "name": "nextjs-template",
  "private": true,
  "type": "module",
  "scripts": {
    "email": "email",
    "build": "next build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "db:push": "cd ./packages/db/ && bun db:push",
    "test:db": "ts-node --esm test-simple.ts",
    "test:query": "ts-node --esm test-query.ts",
    "dev": "next dev",
    "dev:web": "turbo dev --parallel --filter !stripe",
    "format": "turbo format --continue -- --cache --cache-location='node_modules/.cache/.prettiercache' --ignore-path='../../.gitignore'",
    "format:fix": "turbo format --continue -- --write --cache --cache-location='node_modules/.cache/.prettiercache' --ignore-path='../../.gitignore'",
    "lint": "turbo lint -- --quiet -- --cache --cache-location 'node_modules/.cache/.eslintcache' && manypkg check",
    "lint:fix": "turbo lint --continue -- --fix --cache --cache-location 'node_modules/.cache/.eslintcache' ",
    "typecheck": "turbo typecheck",
    "check-deps": "check-dependency-version-consistency .",
    "gen": "turbo gen --config 'turbo/generators/config.ts'"
  },
  "devDependencies": {
    "@react-email/preview-server": "^4.3.0",
    "@turbo/gen": "1.13.3",
    "@types/react": "19.2.2",
    "check-dependency-version-consistency": "4.1.0",
    "prettier": "3.2.5",
    "tailwind-config-viewer": "^2.0.4",
    "ts-node": "^10.9.2",
    "turbo": "1.13.3",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.12.0",
    "@react-email/components": "^0.5.7",
    "dotenv": "^17.2.3",
    "h3": "^2.0.1-rc.2",
    "kysely": "^0.28.8",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "prisma": "^5.12.0",
    "react-email": "^4.3.1",
    "resend": "^6.1.2"
  }
}
